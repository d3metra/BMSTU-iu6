Program DZ1;
{$APPTYPE CONSOLE}
uses
  SysUtils;

Var y:real;
Begin
  WriteLn('До преобразований y=',y:20:16);
  y:= 1;
  y:= y/6000 ;
  y:= exp(y);                  {y = e^x}
  y:= sqrt(y);                 {Квадратный корень}
  y:= y / 14;
  y:= 14*y;
  y:= sqr(y);                  {y = y^2}
  y:= ln(y);
  y:= 6000*y;
  WriteLn('После преобразований =', y:20:16);
  Readln;
End.
