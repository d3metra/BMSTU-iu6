program DZ2_nomer4;
{$APPTYPE CONSOLE}

uses System.SysUtils, RFun;

var x: real;
  function F1 (x: real): real;
    begin
       Result := sqr(ln(x)) - 1;
    end;
  function F2 (x: real): real;
    begin
       Result := sin(x) + cos(x);
    end;

begin
writeln ('ћинимальный положительный корень уравнени€ sqr(ln(x)) = 1');
ROOT (F1);
writeln ('ћинимальный положительный корень уравнени€ sin(x) + cos(x) = 0');
ROOT (F2);
readln;
end.
